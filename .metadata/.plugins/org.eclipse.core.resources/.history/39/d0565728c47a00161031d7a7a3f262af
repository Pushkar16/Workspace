package leet;

public class ReverseInteger {
	 public int reverse(int x) {
		 int i=0;
		 do
		 {
			i=i*10+(x%10);
			x=x/10;
			System.out.println(x);
		 }while(x>0);
		return i;
	        
	    }
	 public static void main(String[] args)
	 {
		 ReverseInteger ri=new ReverseInteger();
		 int a=ri.reverse(123);
		 System.out.println(a);
	 }
}
