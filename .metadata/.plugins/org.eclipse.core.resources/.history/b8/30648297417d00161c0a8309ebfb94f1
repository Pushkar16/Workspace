package leet;

import java.util.Map;
import java.util.TreeMap;

public class IntegerToRoman {
	public  String intToRoman(int num) {
		Map<Integer, String> mp=new TreeMap();
        mp.put(1, "I");
        mp.put(5, "V");
        mp.put(10, "x");
        mp.put(50, "L");
        mp.put(100, "C");
        mp.put(500, "D");
        mp.put(1000,"M");
                
		return null;
	        
	    }

}
