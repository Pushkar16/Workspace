package leet;

import java.util.ArrayList;
import java.util.Comparator;
import java.util.List;

public class Threesome {
	 public List<List<Integer>> threeSum(int[] nums) {
		 boolean b=false;
		 List<List<Integer>> list= new ArrayList<List<Integer>>();
		 for(int i=0;i<nums.length;i++)
			 for(int j=i+1;j<nums.length;j++)
				 for(int k=j+1;k<nums.length;k++)
				 {
					 if(i==j||j==k||k==i)
					 {
						 continue;
					 }
					 else{
						 List<Integer> l=new ArrayList<Integer>();
						 Comparator<Integer> c = null;
						 l.add(nums[i]);
						 l.add(nums[j]);
						 l.add(nums[k]);
						 l.sort(c);
						// System.out.println(l);
						 b=isZero(l);
						 if(b==true)
						 {
							if(isUnique(list,l)) 
						  list.add(l);
						 }
					 }
				 }
		return list;
	        
	    }
	 private boolean isUnique(List<List<Integer>> list, List<Integer> l) {
		 int[] a = new int[l.size()] ;
		 int b[]=new int[l.size()];
		 for(int i=0;i<l.size();i++)
		 {
			 a[i]=l.get(i);
		 }
		 System.out.println("a is");
		 for(int j:a)
		 System.out.print(j);
		 System.out.println();
		 for(List<Integer> l1: list)
		 {
			 for(int i=0;i<l1.size();i++)
			 {
				 b[i]=l1.get(i);
			 }
			 System.out.println("b is");
			 for(int k:b)
			 System.out.print(k);
			 System.out.println();
			 if(a.equals(b))
					return false;
			 
		 }
		 
		 return true;
		
		
	}
	public boolean isZero(List<Integer> a)
	 {
		int sum=0;
		for(int i:a)
		{
			sum=sum+i;
		}
		if(sum==0)
			return true;
		return false;
		 
	 }
	 public static void main(String[] args)
	 {
		 Threesome th=new Threesome();
		 List<List<Integer>> list= new ArrayList<List<Integer>>();
		 int nums[]={-1, 0, 1, 2, -1, -4};
		 list=th.threeSum(nums);
		 /*for(List<Integer> l:list)
		 {
			 for(int a:l)
			 {
				 System.out.print(a);
			 }
			 System.out.println("");
		 }*/
		 
	 }

}
